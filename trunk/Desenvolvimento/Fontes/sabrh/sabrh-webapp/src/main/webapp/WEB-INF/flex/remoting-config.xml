<?xml version="1.0" encoding="UTF-8"?>
<service id="remoting-service" class="flex.messaging.services.RemotingService">

	<adapters>
		<adapter-definition id="java-object"
			class="flex.messaging.services.remoting.adapters.JavaAdapter"
			default="true" />
	</adapters>

	<default-channels>
		<channel ref="my-amf" />
	</default-channels>

	<destination id="UsuarioService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/UsuarioService/remote</source>
		</properties>
	</destination>

	<destination id="MunicipioService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/MunicipioService/remote</source>
		</properties>
	</destination>

	<destination id="EstadoService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/EstadoService/remote</source>
		</properties>
	</destination>

	<destination id="PerfilService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/PerfilService/remote</source>
		</properties>
	</destination>

	<destination id="StatusService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/StatusService/remote</source>
		</properties>
	</destination>

	<destination id="PropriedadeService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/PropriedadeService/remote</source>
		</properties>
	</destination>

	<destination id="AnimalService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/AnimalService/remote</source>
		</properties>
	</destination>

	<destination id="ClassificacaoLinearService">
		<properties>
			<factory>ejb3</factory>
			<source>sabrh-ear-1.0/ClassificacaoLinearService/remote</source>
		</properties>
	</destination>

</service>